{
  "name": "orchid/platform",
  "description": "The Laravel CMS for website.",
  "keywords": [
    "framework",
    "laravel",
    "orchid",
    "crud",
    "cms",
    "cmf",
    "access",
    "alert",
    "attributes",
    "log",
    "menu",
    "scaffold",
    "settings",
    "tags",
    "widget",
    "website",
    "user",
    "editor",
    "permissions",
    "dashboard",
    "active",
    "admin",
    "platform"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://theorchid.github.io/",
  "support": {
    "issues": "https://github.com/theOrchid/Platform/issues",
    "source": "https://github.com/theOrchid/Platform"
  },
  "authors": [
    {
      "name": "Alexandr Chernyaev",
      "email": "bliz48rus@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "laravel/framework": "5.*",
    "watson/active": "^2.0",
    "intervention/image": "^2.3",
    "cartalyst/tags": "^4.0",
    "cviebrock/eloquent-sluggable": "^4.2",
    "spatie/laravel-backup": "4.*",
    "psr/log": "~1.0",
    "orchid/defender": "dev-master",
    "orchid/settings": "dev-master",
    "orchid/alert": "dev-master",
    "orchid/widget": "dev-master",
    "orchid/log" : "dev-master"
  },
  "require-dev": {
    "mockery/mockery": "~0.9.4",
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "Orchid\\": "src/Orchid"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Orchid\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Orchid\\Providers\\FoundationServiceProvider"
      ],
      "aliases": {
        "Dashboard": "Orchid\\Facades\\Dashboard"
      }
    }
  },
  "suggest": {
    "orchid/socket": "Laravel library for asynchronously serving WebSockets."
  },
  "minimum-stability": "alpha",
  "prefer-stable": true
}
