{
  "name": "orchid/platform",
  "description": "Orchid Platform application provides a very flexible and extensible way of building your custom application. ",
  "keywords": [
    "framework",
    "laravel",
    "orchid",
    "crud",
    "cms",
    "cmf",
    "access",
    "alert",
    "attributes",
    "log",
    "menu",
    "scaffold",
    "seo",
    "settings",
    "tags",
    "widget",
    "website",
    "user",
    "editor",
    "permissions",
    "dashboard",
    "active",
    "admin",
    "bootstrap",
    "platform"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "http://orchid.ga",
  "authors": [
    {
      "name": "Alexandr Chernyaev",
      "email": "bliz48rus@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.*",
    "watson/active": "^2.0",
    "intervention/image": "^2.3",
    "cartalyst/tags": "^3.0",
    "spatie/laravel-backup": "4.*",
    "laravel/scout": "^2.0",
    "elasticsearch/elasticsearch": "^5.0",
    "algolia/algoliasearch-client-php": "^1.10",
    "psr/log": "~1.0"
  },
  "autoload": {
    "files": [
      "src/Settings/helpers.php",
      "src/Alert/helpers.php",
      "src/LogViewer/helpers.php"
    ],
    "psr-4": {
      "Orchid\\Access\\": "src/Access",
      "Orchid\\Alert\\": "src/Alert",
      "Orchid\\Field\\": "src/Field",
      "Orchid\\Forms\\": "src/Forms",
      "Orchid\\Foundation\\": "src/Foundation",
      "Orchid\\Image\\": "src/Image",
      "Orchid\\Installer\\": "src/Installer",
      "Orchid\\LogViewer\\": "src/LogViewer",
      "Orchid\\Menu\\": "src/Menu",
      "Orchid\\Presenter\\": "src/Presenter",
      "Orchid\\Search\\": "src/Search",
      "Orchid\\Settings\\": "src/Settings",
      "Orchid\\Type\\": "src/Type",
      "Orchid\\Widget\\": "src/Widget"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "alpha",
  "prefer-stable": true
}
